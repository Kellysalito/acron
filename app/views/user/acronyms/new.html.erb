<br />
<div class="booyah-box col-10 offset-1">
  <h1>Add a new acronym</h1>
   <%= simple_form_for @acronym, url: user_acronyms_path do |f| %>
    <%= f.input :acronym %>
    <%= f.input :description %>
    <br />
    <%= f.submit "Create", class: 'btn btn-primary' %>
    <br />
  <% end %>
  <br />
</div>